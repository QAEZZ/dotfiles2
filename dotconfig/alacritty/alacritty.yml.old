# Configuration for Alacritty, the GPU enhanced terminal emulator.

window:
  padding:
    x: 15 
    y: 15    
  dynamic_padding: true
  opacity: 0.75
font:
  size: 15

##########################
#  OCEAN-DARK COLOR SCHEME
##########################

colors:
  primary:
    background: '#101721'
    foreground: '#E5E9F0'
    dim_foreground: '#43586A'
    dim_background: '#303741'

  normal:
    black: '#101721'
    red: '#8B5252'
    green: '#548F7F'
    yellow: '#BAA285'
    blue: '#43586A'
    magenta: '#8B6191'
    cyan: '#6B8FA3'
    white: '#E5E9F0'

  bright:
    black: '#303741'
    red: '#AF7575'
    green: '#74AF9F'
    yellow: '#C9B19B'
    blue: '#546B7F'
    magenta: '#AF8AA3'
    cyan: '#7FA1B0'
    white: '#E5E9F0'

  dim:
    black: '#000000'
    red: '#7A4444'
    green: '#346F5F'
    yellow: '#BAA285'
    blue: '#23384A'
    magenta: '#6B4171'
    cyan: '#4B6F83'
    white: '#E5E9F0'




